{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "env": {
    "FIREBASE_PRIVATE_KEY": "@firebase-private-key",
    "FIREBASE_CLIENT_EMAIL": "@firebase-client-email"
  },
  "headers": [
    {
      "source": "/api/github",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=120, stale-while-revalidate=60"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/wp-admin",
      "destination": "https://www.youtube.com/watch?v=2ZIpFytCSVc"
    },
    {
      "source": "/wp-login.php",
      "destination": "https://www.youtube.com/watch?v=2ZIpFytCSVc"
    },
    {
      "source": "/.env",
      "destination": "https://www.youtube.com/watch?v=2ZIpFytCSVc"
    }
  ]
}
